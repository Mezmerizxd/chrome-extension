{
  "manifest_version": 3,
  "name": "Social Club Extension",
  "version": "1.0",
  "description": "Social Club Things",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": ["webRequest", "tabs", "activeTab", "storage", "cookies"],
  "host_permissions": ["*://*.rockstargames.com/*"]
}
